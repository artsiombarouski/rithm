{
  "name": "movies",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "check-typescript": "tsc --noEmit",
    "check-eslint": "eslint './src/**/*{js,ts,jsx,tsx}'",
    "lint": "yarn check-typescript && yarn check-eslint",
    "release": "standard-version",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint './src/**/*{js,ts,jsx,tsx}' --fix",
      "prettier --write './src/**/*{js,ts,jsx,tsx}'"
    ]
  },
  "keywords": [
    "react-native",
    "expo",
    "typescript",
    "template"
  ],
  "author": "Artsiom Barouski",
  "license": "MIT",
  "dependencies": {
    "@artsiombarouski/rn-components": "*",
    "@artsiombarouski/rn-expo-router-service": "*",
    "@artsiombarouski/rn-form": "*",
    "@artsiombarouski/rn-resources": "*",
    "@artsiombarouski/rn-services": "*",
    "@artsiombarouski/rn-user-store-service": "*",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/webpack-config": "^18.0.1",
    "@react-navigation/drawer": "^6.6.2",
    "@types/react": "~18.0.27",
    "expo": "~48.0.15",
    "expo-constants": "~14.2.1",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-router": "^1.5.3",
    "expo-status-bar": "~1.4.4",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
    "mobx": "^6.9.0",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-paper": "^5.8.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.10",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.39.0",
    "eslint-config-universe": "^11.2.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0"
  }
}
